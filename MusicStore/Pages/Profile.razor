@page "/Profile"


<body class="BodyProfile">
<div class="wrapper">
    <div class="profile">
        <div class="content">
            <h1>Edit Profile</h1>
            <form action="">
                    <!-- Name -->
                <fieldset>
                    <div class="grid-35">
                        <label for="fname">First Name</label>
                    </div>
                    <div class="grid-65">
                        <input type="text" id="fname" tabindex="1" />
                    </div>
                </fieldset>
                <fieldset>
                    <div class="grid-35">
                        <label for="lname">Last Name</label>
                    </div>
                    <div class="grid-65">
                        <input type="text" id="lname" tabindex="2" />
                    </div>
                </fieldset>
                <!-- Email -->
                <fieldset>
                    <div class="grid-35">
                        <label for="email">Email Address</label>
                    </div>
                    <div class="grid-65">
                        <input type="email" id="email" tabindex="6" />
                    </div>
                </fieldset>

            </form>
        </div>
</div>
</div>
</body>
